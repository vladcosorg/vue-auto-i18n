{
  "name": "vue-auto-i18n",
  "description": "Fully automatic internationalization plugin for vue.js integrated with vue-i18n",
  "author": "chetzof",
  "bugs": "https://github.com/chetzof/vue-auto-i18n/issues",
  "dependencies": {
    "@vitalets/google-translate-api": "^5.0.0",
    "flat": "^5.0.2",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "node-fetch": "^2.6.1",
    "recursive-diff": "^1.0.8"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/flat": "^5.0.1",
    "@types/fs-extra": "^9.0.5",
    "@types/jest": "^26.0.18",
    "@types/lodash": "^4.14.167",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@vue/test-utils": "^1.1.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "fixpack": "^4.0.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.6",
    "jest": "^26.0.0",
    "nock": "^13.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "vue": "^2.0.0",
    "vue-i18n": "^8.0.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/chetzof/vue-auto-i18n",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "keywords": [
    "i18n",
    "internationalization",
    "vue",
    "vue.js",
    "vuejs"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "vue-auto-i18n.esm.js",
  "peerDependencies": {
    "vue": "^2.0.0",
    "vue-i18n": "^8.0.0"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "overrides": [
      {
        "files": ".editorconfig",
        "options": {
          "parser": "yaml"
        }
      },
      {
        "files": "LICENSE",
        "options": {
          "parser": "markdown"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chetzof/vue-auto-i18n.git"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build --tsconfig ./tsconfig.build.json",
    "postbuild": "run-p build:copy-files build:generate-supported-languages",
    "commit": "cz",
    "fixpack": "fixpack",
    "format": "eslint --cache --ext .js,.jsx,.ts,.tsx,.json --fix ./ >/dev/null 2>&1 || true",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  ./",
    "size": "size-limit",
    "start": "run-p postbuild watch",
    "watch": "npm run postbuild && tsdx watch --noClean --tsconfig ./tsconfig.build.json",
    "test": "tsdx test --passWithNoTests",
    "test--watch": "tsdx test --watch",
    "build:copy-files": "cp package.json dist/package.json && cp yarn.lock dist/yarn.lock && cp README.md dist/README.md",
    "build:generate-supported-languages": "ts-node bin/generate-supported-languages.ts"
  },
  "size-limit": [
    {
      "path": "dist/vue-auto-i18n.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vue-auto-i18n.esm.js",
      "limit": "10 KB"
    }
  ],
  "typings": "index.d.ts"
}
